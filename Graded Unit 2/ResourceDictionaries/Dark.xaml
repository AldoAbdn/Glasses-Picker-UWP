    xmlns:local="using:Graded_Unit_2.ResourceDictionaries">
    <SolidColorBrush x:Key="ThemeText" Color="White"/>
    <SolidColorBrush x:Key="ThemeMenuItemUnselected" Color="DarkGray"/>
